CFLAGS	Makefile	/^CFLAGS = -g -Wall -I$(DIR_INC) $/;"	m
CROSS_COMPILE	Makefile	/^CROSS_COMPILE = $/;"	m
DIR_APP	Makefile	/^DIR_APP = \/home\/goeful\/app$/;"	m
DIR_BIN	Makefile	/^DIR_BIN = .\/bin$/;"	m
DIR_INC	Makefile	/^DIR_INC = .\/inc$/;"	m
DIR_LIB	Makefile	/^DIR_LIB = .\/libs$/;"	m
DIR_SRC	Makefile	/^DIR_SRC = .\/src\/gtest.c$/;"	m
KERNELDIR	Makefile	/^KERNELDIR ?= \/home\/goeful\/work\/kernel$/;"	m
LDFLAGS	Makefile	/^LDFLAGS := -L$(DIR_LIB)$/;"	m
MAX_PATH	src/mbase64.c	9;"	d	file:
MDEFINE	Makefile	/^MDEFINE :=$/;"	m
OBJECTS	Makefile	/^OBJECTS := $(patsubst %.c,%.o,$(DIR_SRC))$/;"	m
OBJECTS	Makefile	/^OBJECTS := $(patsubst %.c,%.o,$(wildcard $(DIR_SRC)\/*.c))$/;"	m
PROJECT_NAME	Makefile	/^PROJECT_NAME = app$/;"	m
PROJECT_PWD	Makefile	/^PROJECT_PWD = $(shell pwd)$/;"	m
TARGET	Makefile	/^TARGET = $(DIR_BIN)\/$(PROJECT_NAME)$(shell TZ=CST date -u "+%Y%m%d_%H%M%S")__$(VERSION)$/;"	m
TARGET	Makefile	/^TARGET = $(DIR_BIN)\/$(PROJECT_NAME)_$(VERSION)$/;"	m
TIME	Makefile	/^TIME = Build on $(shell date)$/;"	m
VERSION	Makefile	/^VERSION = 1.0$/;"	m
__MBASE64_H__	inc/mbase64.h	2;"	d
base64_decode	src/gtest.c	/^int base64_decode(char *in_str, int in_len, char *out_str)$/;"	f
base64_decode	src/mbase64.c	/^int base64_decode( const char * base64, unsigned char * bindata )$/;"	f
base64_encode	src/gtest.c	/^int base64_encode(char *in_str, int in_len, char *out_str)$/;"	f
base64_encode	src/mbase64.c	/^char * base64_encode( const unsigned char * bindata, char * base64, int binlength )$/;"	f
base64char	src/mbase64.c	/^const char * base64char = "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+\/";$/;"	v
decode	src/mbase64.c	/^void decode(FILE * fp_in, FILE * fp_out)$/;"	f
encode	src/mbase64.c	/^void encode(FILE * fp_in, FILE * fp_out)$/;"	f
main	src/gtest.c	/^int main(int argc, char *argv[])$/;"	f
main	src/test.c	/^int main(int argc, char *argv[])$/;"	f
